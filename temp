function get_data(query){
    get(child(dbRef, query)).then((snapshot) => {
        if (snapshot.exists()) {
            return snapshot.val() ;
        } else {
            console.log("No data available");
            return "";
        }
        }).catch((error) => {
            console.error(error);
    });
}

// test get_data()
const query = 'users/rWwhXguU1kVfdmcdW5S39He4tCq1/email'
const data = get_data(query)
console.log(data)
